{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Lambda note",
	"patterns": [
		{"include": "#block"},
		{"include": "#inline"}
	],
	"repository": {
		"block": {
			"patterns": [
				{"include": "#block-extension"},
				{"include": "#heading"},
				{"include": "#metadata"},
				{"include": "#divider"}
			]
		},
		"inline": {
			"patterns": [
				{"include": "#inline-extension"},
				{"include": "#styled-expression"}
			]
		}
	},
	"scopeName": "source.lambdanote"
}