{
    "name": "lambda-note-syntax",
    "displayName": "λnote syntax",
    "description": "λnote syntax support",
    "version": "0.0.1",
    "license": "MIT",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/adelhult/lambda-note"
    },
    "homepage": "https://github.com/adelhult/lambda-note",
    "keywords": ["Lambda note", "markup", "syntax"],
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lambdanote",
            "aliases": ["λnote", "Lambda note", "lambdanote"],
            "extensions": [".ln"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lambdanote",
            "scopeName": "source.lambdanote",
            "path": "./syntaxes/lambdanote.tmLanguage.json"
        }]
    }
}